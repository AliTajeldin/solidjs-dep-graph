{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "types": ["vite/client"],
    "baseUrl": ".",
    "isolatedModules": true,
    "paths": {
      "solidjs-dep-graph": ["../lib/src/index.tsx"],
    }
  },
  "exclude": ["node_modules"]
}
