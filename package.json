{
  "name": "xxx-solidjs-dep-graph-xxx",
  "version": "0.0.1",
  "description": "mono-repo wrapper.  The actual package.json is in ./lib dir",
  "license": "APACHE",
  "scripts": {
    "dev": "cd ./demo && pnpm dev",
    "build": "pnpm -r build",
    "preview": "cd ./demo && pnpm preview",
    "clean": "rm -rf dist demo/dist lib/dist",
    "clobber": "rm -rf node_modules dist && pnpm run -r clobber",
    "release": "cd ./lib && pnpm build && pnpm release",
    "deploy" : "cd ./demo && pnpm run deploy"
  },
